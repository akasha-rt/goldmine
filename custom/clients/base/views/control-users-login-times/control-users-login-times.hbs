<div class="row-fluid">
    <div class="main-pane span8">
        <div class="headerpane">
            <h1>
                <span class="record-cell" data-type="avatar" data-name="picture">
                    <span class="table-cell-wrapper">
                        <p><i class="fa fa-users" aria-hidden="true"
                                style="font-size: x-large; margin-top: 30% !important;"></i></p>
                    </span>
                </span>
                <span class="record-cell" data-type="name" data-name="name" style="width: 715px;">
                    <div class="ellipsis_inline" data-placement="bottom" title="Control Users Login">
                        <p style="font-weight:bold;">{{str 'LBL_CONTROL_USERS_LOGIN'}}</p>
                    </div>
                </span>
                <div class="btn-toolbar pull-right">
                    <span sfuuid="1136" class="detail" tabindex="-1">
                        <button class="btn btn-default" name="cancel">{{str 'LBL_HEADERPANE_BUTTON_CANCEL'}}</button>
                        <input class="btn btn-primary" type="button" id="saveUsersLoginTimes" value="Save">
                    </span>
                </div>
            </h1>
        </div>
        <div class="record">
            <div class="row-fluid panel_body">
                <div class="span6 record-cell edit" data-type="textarea" data-name="title" rel="">
                    <div class="record-label">
                        {{str 'LBL_USERS'}}
                    </div>

                    <span class="normal index" data-fieldname="title" data-index="">
                        <span sfuuid="257" class="edit">
                            <select class='users_select' style='width:100%' multiple='multiple'
                                placeholder='Select users'></select>
                        </span>
                    </span>
                </div>
                <div class="span6 record-cell edit">
                </div>
            </div>
            <div class="row-fluid panel_body">
                <div class="span4 record-cell" data-type="datetime">
                    <div class="record-label" style="margin-bottom:20px; margin-top:20px;font-size: 14px;">
                        {{str 'LBL_PREVENT_USERS_LOGIN'}}
                    </div>
                    <div class="normal index" style="margin-left:10px;">
                        <input type="checkbox" id="controlUserLoginDays" name="controlUserLoginDays">
                    </div>
                </div>
                <div class="span4 record-cell" data-type="datetime">
                    <div class="record-label" style="margin-top:10px; margin-bottom:10px;">{{str 'LBL_START_DATE'}}
                    </div>
                    <div class="normal index">
                        <input type="text" class="span2 ui-timepicker-input" placeholder="hh:mm" id="tp1"
                            rel="timepicker" autocomplete="off" style="width:80px">
                    </div>
                </div>
                <div class="span4 record-cell" data-type="datetime">
                    <div class="record-label" style="margin-top:10px; margin-bottom:10px;">{{str 'LBL_END_DATE'}}</div>
                    <div class="normal index">
                        <input type="text" class="span2 ui-timepicker-input" placeholder="hh:mm" id="tp2"
                            rel="timepicker" autocomplete="off" style="width:80px">
                    </div>
                </div>
            </div>
            <div class="row-fluid panel_body">
                <div class="span12" id="presetInfoContainer"
                    style="display: none; padding: 0px;width: 100%;margin-top:50px;">
                    <div class="row-fluid record-panel" data-panelname="panel_body" style="margin-left:-15px;">
                        <div class="row-fluid record-panel-header panel-active" id="panelToggle" href="#">
                            <div class="record-label"
                                style="margin-bottom:20px; margin-top:20px;font-size: 14px; margin-left:20px;">
                                {{str 'LBL_SAVED_DATA'}}
                            </div>
                        </div>
                        <div class="row-fluid panel-collapse collapse in" style="display: block;">
                            <table class="table table-striped table-bordered table-condensed">
                                <thead style="background: gainsboro;">
                                    <tr>
                                        <th style="text-align:center;width:365px;"><span>{{str 'LBL_USER_NAME'}}</span>
                                        </th>
                                        <th style="text-align:center;width:365px;"><span>{{str 'LBL_START_DATE'}}</span>
                                        </th>
                                        <th style="text-align:center;width:365px;"><span>{{str 'LBL_END_DATE'}}</span>
                                        </th>
                                        <th style="text-align:center;width:365px;">
                                            <span>{{str 'LBL_REMOVE_USER_CONF'}}</span>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody id="panelCollapserBody">
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="side sidebar-content span4">
        <div
            style="background-color:#F3F3F3; height: 5.8%;;border-width: 1px; border-bottom-style: solid; border-bottom-color: gainsboro; border-left-style: solid; border-left-color: gainsboro;">
        </div>
        <div class="record-label" data-name="title"
            style="height:30px;margin-top: -50px;margin-left: 15px;position: absolute;">
            <h4>{{str 'LBL_HELP'}}</h4>
        </div>
        <div data-type="text" data-name="title"
            style="background:white;border-style:solid;border-width:1px;border-color:gainsboro; margin-top:15px; margin-left:15px; margin-right:15px;">
            <span class="normal index" data-fieldname="title" data-index="">
                <span sfuuid="39" class="edit" id="headerContainer" style="margin-top:-15px;">
                    <div class="headerpane"
                        style="width: 100.1%; position: static;min-height: 60px;border-style: solid;border-width: 1px;border-color: gainsboro;">
                        <div style="display:inline-flex;">
                            <h4 style="padding-left:10px; margin-right:10px;"><i class="fa fa-info-circle"></i></h4>
                            <h4>{{str 'LBL_CONTROL_USERS_LOGIN_TIPS'}}</h4>
                        </div>
                    </div>
                </span>
                <div sfuuid="39" class="edit" id="recordContainer" style="margin-top: 20px;min-height: 80px;">
                    <h1 id="previewFieldLabel" style="margin-left: 16px; font-weight: bold;"></h1>
                    <div class="btn-list" id="ControlLoginTips" style="margin-right: 10px;">
                        <h1 style="margin-left: 2%;"><b>{{str 'LBL_ELEMMENTS'}}</b></h1>
                        <table class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>{{str 'LBL_ELEMMENT'}}</th>
                                    <th>{{str 'LBL_DESCRIPTION'}}</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        <p>{{str 'LBL_USERS'}}</p>
                                        <div style="display:inline-flex">
                                            <input type="text" placeholder="vagrant admin" disabled=""
                                                style="width: 150px;margin-right: 10px;">

                                        </div>
                                    </td>
                                    <td>
                                        {{str 'LBL_USERS_TIPS'}}
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>{{str 'LBL_PREVENT_USERS_LOGIN'}}</p>
                                        <div style="display:inline-flex">
                                            <input type="checkbox" id="" name="" checked disabled="">
                                        </div>
                                    </td>
                                    <td>
                                        {{str 'LBL_PREVENT_USERS_LOGIN_TIPS'}}
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>{{str 'LBL_START_DATE'}}</p>
                                        <div style="display:inline-flex">
                                            <input type="text" placeholder="8:00am" disabled="" style="width: 50px;">
                                        </div>
                                    </td>
                                    <td>
                                        {{str 'LBL_START_DATE_TIPS'}}
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>{{str 'LBL_END_DATE'}}</p>
                                        <div style="display:inline-flex">
                                            <input type="text" placeholder="5:00pm" disabled="" style="width: 50px;">
                                        </div>
                                    </td>
                                    <td>
                                        {{str 'LBL_END_DATE_TIPS'}}
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </span>
        </div>
    </div>
</div>