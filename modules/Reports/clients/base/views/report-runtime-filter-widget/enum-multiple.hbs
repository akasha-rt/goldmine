<div class="flex flex-row items-center m-2 ml-2.5 mr-2.5">
    <input class="w-full" type="text" value="{{data._searchTerm}}" placeholder="{{str LBL_SEARCH}}" data-fieldname="search-multiple">
</div>
<div class="report-multiselect-options">
    {{#each data._inputValue}}
        <div class="flex flex-row items-center m-2 ml-2.5 mr-2.5">
            <input class="mr-2.5" type="checkbox" value="{{@key}}" data-fieldname="enum-multiple" checked>
            {{this}}
        </div>
    {{/each}}
    {{#each data._inputValue1}}
        <div class="flex flex-row items-center m-2 ml-2.5 mr-2.5">
            <input class="mr-2.5" type="checkbox" value="{{@key}}" data-fieldname="enum-multiple">
            {{this}}
        </div>
    {{/each}}
</div>
