<style>
    #content {
        overflow: scroll;
        background-color: #fff !important;
        margin: 0;
        font-family: "Nunito", sans-serif;
        font-size: 0.9rem;
        font-weight: 400;
        line-height: 1.6;
    }

    .listing-portal-configuration {
        padding: 10px;
    }

    .headerpane {
        left: 0 !important;
    }

    .headerpane .record-cell, .headerpane .ellipsis_inline {
        max-width: 300px !important;
        width: 300px !important;
    }

    .record-cell-cstm {
        position: relative;
        width: 100%;
        min-height: 28px;
        box-sizing: border-box;
        font-size: 14px;
        z-index: 1;
    }

    iframe {
        background: white;
    }

    .loader {
        border: 16px solid #f3f3f3; /* Light grey */
        border-top: 16px solid #3498db; /* Blue */
        border-radius: 50%;
        width: 120px;
        height: 120px;
        animation: spin 2s linear infinite;
        position: center !important;
    }

    @keyframes spin {
        0% {
            transform: rotate(0deg);
        }
        100% {
            transform: rotate(360deg);
        }
    }

    .center {
        float: none;
        margin-left: auto;
        margin-right: auto;
    }

</style>
<div class="container">
    <div class="row-fluid">
        <div class="span12">
            <hr>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <div class="headerpane">
                <h1>
                    <span class="record-cell" data-type="label">
                        <span class="table-cell-wrapper">
                            <span class="index" data-fieldname="title">
                                <span class="list-headerpane">
                                    <div class="ellipsis_inline" data-placement="bottom"
                                         data-original-title="{{str 'LBL_CONFIGURE_TIME_SAVER'
                                                                    'E38_DuplicateFinderProcess'}}">
                                        {{str 'LBL_CONFIGURE_TIME_SAVER' 'E38_DuplicateFinderProcess'}}
                                    </div>
                                </span>
                            </span>
                        </span>
                    </span>
                    <div class="btn-toolbar pull-right dropdown">
                        <button class="btn btn-invisible pull-right" id="closeConfiguration">
                            {{str 'LBL_CLOSE' 'E38_DuplicateFinderProcess'}}
                        </button>
                    </div>
                </h1>
            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span2 offset2">{{str 'LBL_LICENSE' 'E38_DuplicateFinderProcess'}}:</div>
        <div class="span4">
            <input id="license-key" type="text" class="record-cell-cstm" value="{{license}}">
        </div>
        <div class="span2 offset1">
            <button id="update-license" class="btn btn-link">
                {{str 'LBL_UPDATE_LICENSE' 'E38_DuplicateFinderProcess'}}
            </button>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <hr>
        </div>
    </div>
    <div class="row-fluid">
        <div id="loader"></div>
        <div class="span10 offset1">
            {{#if usersLoaded}}
                <iframe id="license-frame" src="https://mls.38elements.com/configuration/{{license}}" width="800"
                        height="800" frameBorder="0"></iframe>
            {{/if}}
        </div>
    </div>
</div>
